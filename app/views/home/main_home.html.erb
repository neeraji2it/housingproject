      <nav class="main-nav"></nav>      
    </div>

    <div class="all">
      <div class="slider-wrapper fullscreen">
        <!--<div class="slogo">
            <a href="#"><img src="img/icon-happycake.png" class="logo"></a>
        </div>-->
        <div class="slide_bg" data-stellar-background-ratio="0.3">
          <div class="container">
            

              <h1 style="color:#FFF; "><a href="/"><img src="/assets/logo.png"  class="logo"></a></h1>

              <ul class="nav pull-right"> 
                <table>
                    <th>   
                  <div class="project-type">
                    <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Post Your Requirements</a></div>
                  </div>
                  </th>
                  
                  <th>
                  <div class="project-type">
                    <div class="text-label" ><a href="<%= jointventure_home_index_path %>" class="property-select">List your Property </a></div>
                  </div> 
                  </th>
                  
                  <th>   
                  <div class="project-type">
                    <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Joint Venture</a></div>
                  </div>
                  </th>
                  
                </table>
              </ul>

           <div class="row"  style= "padding: 340px 50px 0px 30px;">
              <div class="col-md-12">
                <div class="text-center">

                  <h4 style="color:#FFF;">Choose Project Type</h4>

                  <div class="project-container">

                    <div class="project-type">
                      <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Agriculture Lands</a></div>
                    </div>


                    <div class="project-type">
                      <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Farm Land</a></div>
                    </div>       


                    <div class="project-type">
                      <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Commercial Land</a></div>
                    </div>

                    <div class="project-type">
                      <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Apartments</a></div>
                    </div>
                    <div class="project-type">
                      <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Barren Land</a></div>
                    </div>
                    <div class="project-type">
                      <div class="text-label"><a href="<%= home_index_path %>" class="property-select">Residential Plots</a></div>
                    </div>

                  </div>




                </div>





              </div>
            </div>
          </div>
        </div>
      </div>


