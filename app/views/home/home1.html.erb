<script>
  $(document).on("click", ".ch-info-back", function() {
    //var address = $(".ch-info-back h3").html();
    var lat = $(this).data("lat");
    var lon = $(this).data("lon");
    var city = $(this).data("city");
    location.href = "/home/land?lat=" + lat + "&lon=" + lon + "&city=" + city;
  });
</script>
